CGROUP	GROUP	TEXT
TEXT	CSEG

liogpset_::
	PUSH	DS
	PUSH	ES
	PUSH	SS
	PUSH	AX
	PUSH	BX
	PUSH	CX
	PUSH	DX
	PUSH	BP
	PUSH	SP
	PUSH	SI
	PUSH	DI

	SHR	AX,4
	ADD	AX,BX
	INC	AX
	MOV	DS,AX

	MOV	BP,SP
	ADD	BP,24

	XOR	BX,BX
	MOV	DS:[0].W,CX	;X
	MOV	DS:[2].W,DX	;Y

	MOV	AX,SS:[BP]
	MOV	DS:[4].B,AL	;Pallet Number
        MOV     AH,01h

	INT	0a6h

	POP	DI
	POP	SI
	POP	SP
	POP	BP
	POP	DX
	POP	CX
	POP	BX
	POP	AX
	POP	SS
	POP	ES
	POP	DS

	RET
